{"nbformat":4,"nbformat_minor":0,"metadata":{"colab":{"name":"08_MEDIAN FILTER.ipynb","provenance":[{"file_id":"1QvtCC9D5YnzaS0UXgZRvDA5HaxmmE7Q6","timestamp":1657875251919}],"mount_file_id":"1ADsofBVk0spFpyHslzswRQ8Z4-n_WuvW","authorship_tag":"ABX9TyOtk48P7sBywQbp7Znak95H"},"kernelspec":{"name":"python3","display_name":"Python 3"},"language_info":{"name":"python"}},"cells":[{"cell_type":"code","execution_count":null,"metadata":{"id":"42CNt1BMpZ-9"},"outputs":[],"source":["import numpy as np\n","import cv2 \n","import random\n","import matplotlib.pyplot as plt\n","from google.colab.patches import cv2_imshow"]},{"cell_type":"code","source":["image = cv2.imread('/content/drive/MyDrive/Colab Notebooks/lenna.png')\n","gs = 0.289*image[:,:,0] + 0.587*image[:,:,1] + 0.114*image[:,:,2]\n","# cv2_imshow(gs)"],"metadata":{"id":"6Gdnqhywr0N7"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["def noise(img: np.array):\n","   row , col = img.shape\n","   number_of_pixels = random.randint(100, 10000)\n","   for i in range(number_of_pixels):\n","     # Picking random x,y coordinate for adding black and white pixels 0,255\n","     y_coord=random.randint(0, img.shape[0]-1)\n","     x_coord=random.randint(0, img.shape[1]-1)\n","     img[y_coord][x_coord] = 255\n","\n","   for i in range(number_of_pixels):\n","     y_coord=random.randint(0, img.shape[0]-1)\n","     x_coord=random.randint(0, img.shape[1]-1)\n","     img[y_coord][x_coord] = 0\n","         \n","   return img"],"metadata":{"id":"-cco6Gh5tfN2"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["def filtered_img(img: np.array) -> np.array:\n","  row, col = img.shape\n","  output_img = np.zeros(shape = [row,col])\n","  for i in range(1, row-1):\n","    for j in range(1,col-1):\n","       temp = [img[i-1, j-1],\n","               img[i-1, j],\n","               img[i-1, j + 1],\n","               img[i, j-1],\n","               img[i, j],\n","               img[i, j + 1],\n","               img[i + 1, j-1],\n","               img[i + 1, j],\n","               img[i + 1, j + 1]]\n","      #  new = sorted(temp)\n","      #  output_img[i,j]= temp[4] \n","       for k in range(len(temp)):\n","         for l in range(0,len(temp)-1):\n","           if (temp[l]>temp[l+1]):\n","             temp[l], temp[l+1] = temp[l+1],temp[l]\n","             output_img[i,j] = temp[4]\n","  return output_img"],"metadata":{"id":"a1NlwhUh1XMB"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["noisy_img = noise(gs)\n","filt = filtered_img((noisy_img))\n","plt.subplot(121), plt.imshow(noisy_img, cmap = 'gray'), plt.title(\"Original Image\")\n","plt.subplot(122), plt.imshow(filt, cmap = 'gray'), plt.title(\"Original Image\")"],"metadata":{"id":"yrUthtR1PJ_2"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":
